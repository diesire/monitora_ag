<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
    http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">
	<persistence-unit name="monitora_ag" transaction-type="RESOURCE_LOCAL">
		<mapping-file>META-INF/orm.xml</mapping-file>
		<class>es.uniovi.miw.monitora.server.model.Agente</class>
		<class>es.uniovi.miw.monitora.server.model.Cliente</class>
		<class>es.uniovi.miw.monitora.server.model.Consulta</class>
		<class>es.uniovi.miw.monitora.server.model.Destino</class>
		<class>es.uniovi.miw.monitora.server.model.InfPlanDest</class>
		<class>es.uniovi.miw.monitora.server.model.InfPlanDestBorrado</class>
		<class>es.uniovi.miw.monitora.server.model.Informe</class>
		<class>es.uniovi.miw.monitora.server.model.InformeConsulta</class>
		<class>es.uniovi.miw.monitora.server.model.InformeTipoDestino</class>
		<class>es.uniovi.miw.monitora.server.model.LineaCron</class>
		<class>es.uniovi.miw.monitora.server.model.Planificacion</class>
		<class>es.uniovi.miw.monitora.server.model.Snapshot</class>
		<class>es.uniovi.miw.monitora.server.model.Tcon1</class>
		<class>es.uniovi.miw.monitora.server.model.TipoDestino</class>
		<class>es.uniovi.miw.monitora.server.model.keys.DestinoPK</class>
		<class>es.uniovi.miw.monitora.server.model.keys.InfPlanDestBorradoPK</class>
		<class>es.uniovi.miw.monitora.server.model.keys.InfPlanDestPK</class>
		<class>es.uniovi.miw.monitora.server.model.keys.InformeConsultaPK</class>
		<class>es.uniovi.miw.monitora.server.model.keys.InformeTipoDestinoPK</class>
		<class>es.uniovi.miw.monitora.server.model.keys.SnapshotPK</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:hsqldb:hsql://localhost:9002/monitoraag" />
			<property name="javax.persistence.jdbc.user" value="SA" />
			<property name="javax.persistence.jdbc.password" value="" />
			<property name="javax.persistence.jdbc.driver" value="org.hsqldb.jdbcDriver" />
			<property name="hibernate.dialect" value="org.hibernate.dialect.HSQLDialect" />
			<property name="hibernate.connection.driver_class" value="org.hsqldb.jdbcDriver" />
		</properties>
	</persistence-unit>
</persistence>
